start:
    mov ax, @data
    mov ds, ax
    mov ax, 13h
    int 10h

    xor ax, ax
    xor dx, dx
    xor bx, bx
    xor si, si
    xor cx, cx
    xor di, di

    call init

; *******************INIT***************
; Note - here order is important, because they save their background here

; **************************************

    push offset oldKeyboardHandlerSegment
    push offset oldKeyboardHandlerOffset
    call setupKeyboardHandler

    push offset previousIRQMask
    call setUpClock

keepAliveLoop:
    jmp keepAliveLoop
exit:
