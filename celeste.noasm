    mov bp, sp
    push bx
    push dx
    push di
    push ax

    mov di, [bp + 8] ; 2d array
    mov bx, [bp + 6] ; row array
    mov dx, [bp + 4] ; current index

    cmp dx, -1
    jnz .continue
    
    mov ax, [di + Array2DElementSize]
    mov dx, [di + Array2DColumns]
    push dx

    inc dx
    mul dx
    add bx, ax

    pop dx

.continue:
    dec dx
    mov [bp + 6], dx

    sub bx, [di + Array2DElementSize]
    mov [bp + 8], bx

