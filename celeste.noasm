proc getRGBFromScreen
    push bp
    mov bp, sp
    push di
    push es
    push ax
    push bx

    mov ax, PRINT_LOCATION
    mov es, ax

    push [bp + 4] ; point x 
    push [bp + 6] ; point y
    call toPosition
    pop di ; di -> position

    mov al, [es:di] ; al -> color index
    mov dx, 3C7h
    out dx, al
    mov dx, 3C9h

    mov ah, 0

    in al, dx
    mov [bp + 4], al ; r

    in al, dx
    mov [bp + 6], al ; g

    in al, dx
    mov [bp + 8], al ; b

    pop bx
    pop ax
    pop es
    pop di
    pop bp
    ret
endp getRGBFromScreen

proc printImageUpscaled
    push bp
    mov bp, sp
    push ax
    push bx
    push es

    mov bx, [bp + 10] ; image pointer
    mov es, [bp + 8]  ; image segment

    mov ax, bx
    add ax, ImageData
    push ax ; image data (colors)
    push es ; image segment
    push [es:bx + ImageWidth]
    push [es:bx + ImageHeight]
    push [bp + 6] ; screen position (left top)
    push [bp + 4] ; scale
    call printImageBySizeUpscaled

    pop es
    pop bx
    pop ax
    pop bp
    ret 8
endp printImageUpscaled

