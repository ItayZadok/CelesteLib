proc goThroughRowUpwards
    push bp
    mov bp, sp
    push bx
    push ax
    push dx
    push di

    mov di, [bp + 8] ; 2d array
    mov bx, [bp + 6] ; row array
    mov dx, [bp + 4] ; current index

    cmp dx, -1
    jnz .continue
    mov dx, [di + Array2DColumns]
    sub bx, [di + Array2DElementSize]
.continue:
    dec dx
    mov [bp + 6], dx

    add bx, [di + Array2DElementSize]
    mov [bp + 8], bx

    pop di
    pop dx
    pop ax
    pop bx
