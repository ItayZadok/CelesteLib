proc clampSizeToScreenBoundsUpscaled
    push bp
    mov bp, sp
    push ax
    push dx
    push bx
    push cx
    push si
    push di

    mov bx, [bp + 10] ; bx -> width
    mov si, [bp + 8]  ; si -> height
    mov di, [bp + 4]  ; di -> multiplier

    push bx
    push di
    call multiply
    pop bx ; bx -> multiplied width

    push si
    push di
    call multiply
    pop si ; si -> multiplied height

    push 0
    push [bp + 6] ; position
    call toXandY
    pop dx ; dx -> x
    pop ax ; ax -> y

    ; if position is out of bounds skip draw
    cmp ax, ScreenHeight
    jge .skipDraw
    cmp dx, ScreenWidth
    jge .skipDraw

    ; Clamp width to screen bounds if necessary
    add dx, bx
    cmp dx, ScreenWidth
    jl .checkHeight

    sub dx, ScreenWidth
    sub bx, dx ; bx -> new width

    push dx
    push di
    call divideModulo
    pop dx
    pop dx
    cmp dx, 0
    jz .checkHeight 

    add bx, di

.checkHeight:
    ; Clamp height to screen bounds if necessary
    add ax, si
    cmp ax, ScreenHeight
    jl .end

    sub ax, ScreenHeight
    sub si, ax ; si -> new height
    add si, di

    push ax
    push di
    call divideModulo
    pop ax
    pop ax
    cmp ax, 0
    jz .checkHeight 

    add si, di

    jmp .end
.skipDraw:
    mov cx, -1
.end:

    push bx
    push di
    call divide
    pop bx

    push si
    push di
    call divide
    pop si

    mov [bp + 6], bx  ; new width
    mov [bp + 8], si  ; new height
    mov [bp + 10], cx ; flag (-1 = skip)

    pop di
    pop si
    pop cx
    pop bx
    pop dx
    pop ax
    pop bp
    ret 2
endp clampSizeToScreenBoundsUpscaled

proc printImageBySizeUpscaled
    push bp
    mov bp, sp
    push di
    push ax
    push bx
    push cx
    push dx
    push si
    push es
    push ds

    mov ax, PRINT_LOCATION
    mov es, ax ; set the print location

    mov bx, [bp + 14] ; image color array
    mov ds, [bp + 12] ; image color segment
    mov dx, [bp + 10] ; image width
    mov cx, [bp + 8]  ; image height
    mov di, [bp + 6]  ; screen start position
    mov ax, [bp + 4]  ; scale multiplier

    push dx
    push cx
    push di
    push ax
    call clampSizeToScreenBoundsUpscaled
    pop dx
    pop cx
    pop si

    cmp si, -1
    je .end

    ; di = start screen position
    ; bx = points the current color in image array
    ; dx = image width
    ; cx = image height counter
    ; si = x counter within current row
    
    xor si, si
.printLoop:
    push cx

    xor ch, ch
    mov cl, [bx]    ; get color
    cmp cl, -1
    je .skipPixel   ; skip if transparent pixel (-1)

    push cx
    push ax
    push ax
    push di
    call printRectBySize

.skipPixel:
    pop cx

    inc bx
    inc si
    
    add di, ax

    cmp si, dx
    jne .printLoop   ; if row not done, continue

    push cx

    mov cx, [bp + 10]
    sub cx, dx
    add bx, cx ; fix color if width changes

    mov cx, ScreenWidth
    sub cx, dx

    push cx
    push ax
    call multiply
    pop cx
    add di, cx ; set new row screen pos by multiplier

    pop cx

    xor si, si       ; reset x counter
    dec cx
    jnz .printLoop   ; repeat for next row
.end:
    pop ds
    pop es
    pop si
    pop dx
    pop cx
    pop bx
    pop ax
    pop di
    pop bp
    ret 12
endp printImageBySizeUpscaled

