proc divideModulo
    push bp
    mov bp, sp
    push ax
    push dx

    mov ax, [bp + 6]

    cwd
    idiv [bp + 4]
    mov [bp + 4], ax ; /
    mov [bp + 6], dx ; %

    pop dx
    pop ax
    pop bp
    ret
endp divideModulo

