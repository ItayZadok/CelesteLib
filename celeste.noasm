    mov dx, [bp + 4] ; current counter (not index)

    cmp dx, -1
    jnz .continue
    mov dx, [bx + ArraySize]
.continue:
    dec dx
    mov [bp + 8], dx

    mov ax, [bx + ArraySize]
    sub ax, dx
    mov dx, ax
    dec dx ; dx -> index

    mov ax, [bx + ArrayElementSize]
    mul dx ; ax = index * element size

    add bx, ArrayData
    add bx, ax
    mov bx, [bx]
    mov [bp + 6], bx

    pop ds
    pop dx
    pop ax
    pop bx
    pop bp
    ret 2
endp goThroughArrayUpwardsValue

