proc updateControllerRotation
    push bp
    mov bp, sp
    push bx
    push es
    push ax
    push cx

    mov bx, [bp + 12] ; bx -> controller offset
    mov es, [bp + 10] ; es -> controller segment
    mov ax, [bp + 8]  ; ax -> rotate speed

    push [bp + 4] ; Left Key
    call isKeyHeld
    pop cx
    cmp cx, 0
    je .skipLeft  ; if pressed Left neg and jmp to update the speed

    neg ax
    jmp .updateRotation
.skipLeft:

    push [bp + 6] ; Right Key
    call isKeyHeld
    pop cx
    cmp cx, 0
    je .skipRight  ;  if pressed Right update the speed
.updateRotation:

    push bx ; controller offset
    push es ; controller segment
    push RotationComponentId
    call getControllerComponent
    pop bx ; bx -> component offset
    pop es ; es -> component segment

    mov cx, [es:bx + RotationComponentCurrentRotation] ; cx -> current rotation
    add cx, ax ; cx -> current rotation + speed

    ; TODO - fix that if it's 0 and we go -30 it sets it to 359 and not 329 (caused due to the clamp)

    push cx ; rotation
    push 0
    push FULL_ROTATION_DEG - 1
    call getInverseClamp
    pop [es:bx + RotationComponentCurrentRotation] ; clamp and set the rotation to the (0-359) range
.skipRight:

    pop cx
    pop ax
    pop es
    pop bx
    pop bp
    ret 10
endp updateControllerRotation

proc applyControllerRotation
    push bp
    mov bp, sp
    push bx
    push es
    push cx
    push ax
    push dx

    mov bx, [bp + 6] ; bx -> controller offset
    mov es, [bp + 4] ; es -> controller segment

    push bx
    push es
    push RotationComponentId
    push RotationComponentCurrentRotation
    call getControllerWordValue
    pop cx ; cx -> rotation

    push bx
    push es
    push RotationComponentId
    push RotationComponentMaxIndex
    call getControllerWordValue
    pop ax ; ax -> max index

    push FULL_ROTATION_DEG
    push ax
    call divide
    pop dx

    push cx
    push dx
    call divide
    pop cx

    dec ax
    push cx
    push ax
    call getMin ; limit cx to not surpass the array
    pop cx

    push bx ; controller offset
    push es ; controller segment
    push cx ; new rotation
    push RenderComponentId
    push RenderComponentIndex
    call setControllerWordValue

    pop dx
    pop ax
    pop cx
    pop es
    pop bx
    pop bp
    ret 4
endp applyControllerRotation

proc getSinus
    push bp
    mov bp, sp
    push bx
    push es
    push ax

    mov bx, [bp + 4] ; rotation

    cmp bx, 180
    jge .negative

    add bx, offset sin_table
    mov ax, seg sin_table
    mov es, ax

    mov bl, [es:bx]
    xor bh, bh

    jmp .end
.negative:

    add bx, offset sin_table
    mov ax, seg sin_table
    mov es, ax

    mov bl, [es:bx]
    xor bh, bh

    neg bx

.end:
    mov [bp + 4], bx

    pop ax
    pop es
    pop bx
    pop bp
    ret
endp getSinus

proc getCosinus
    push bp
    mov bp, sp
    push bx
    push es
    push ax

    mov bx, [bp + 4] ; rotation

    cmp bx, 270
    jge .positive

    cmp bx, 90
    jle .positive

    add bx, offset cos_table
    mov ax, seg cos_table
    mov es, ax

    mov bl, [es:bx]
    xor bh, bh

    neg bx

    jmp .end
.positive:

    add bx, offset cos_table
    mov ax, seg cos_table
    mov es, ax

    mov bl, [es:bx]
    xor bh, bh

.end:
    mov [bp + 4], bx

    pop ax
    pop es
    pop bx
    pop bp
    ret
endp getCosinus

proc moveRotatedController
    push bp
    mov bp, sp
    push bx
    push es
    push cx
    push ax
    push dx
    push si

    mov bx, [bp + 12] ; bx -> controller offset
    mov es, [bp + 10] ; es -> controller segment
    mov si, [bp + 8]  ; si -> speed

    push bx
    push es
    push RotationComponentId
    push RotationComponentCurrentRotation
    call getControllerWordValue
    pop cx ; cx -> current rotation

    push [bp + 4] ; Forward Key
    call isKeyHeld
    pop dx
    cmp dx, 0
    je .skipForward

    push cx
    call getCosinus
    push si
    call divide
    pop ax

    neg ax

    push bx
    push es
    push ax
    push RenderComponentId
    push RenderComponentPositionY
    call addControllerWordValue

    push cx
    call getSinus
    push si
    call divide
    pop ax

    push bx
    push es
    push ax
    push RenderComponentId
    push RenderComponentPositionX
    call addControllerWordValue

.skipForward:

    push [bp + 6] ; Back Key
    call isKeyHeld
    pop dx
    cmp dx, 0
    je .skipBackward

    push cx
    call getCosinus
    push si
    call divide
    pop ax

    push bx
    push es
    push ax
    push RenderComponentId
    push RenderComponentPositionY
    call addControllerWordValue
    push cx
    call getSinus
    push si
    call divide
    pop ax

    neg ax

    push bx
    push es
    push ax
    push RenderComponentId
    push RenderComponentPositionX
    call addControllerWordValue


.skipBackward:

    pop si
    pop dx
    pop ax
    pop cx
    pop es
    pop bx
    pop bp
    ret 10
endp moveRotatedController

proc handleBuffer
    push bp
    mov bp, sp
    push bx
    push es
    push ax
    push si

    mov ax, [bp + 10] ; ax -> input (1 / 0)
    mov si, [bp + 8]  ; si -> buffer duration
    mov bx, [bp + 6]  ; bx -> time since last input offset
    mov es, [bp + 4]  ; es -> time since last input segment

    cmp ax, 0
    je .noInput
    mov [word ptr es:bx], 1 ; if input -> timer = 1
.noInput:

    cmp [word ptr es:bx], si ; if timer reached the duration
    je .end

    inc [word ptr es:bx] ; update the timer (+1)
    mov ax, 1 ; return 1

.end:
    mov [bp + 10], ax ; return 1 / 0

    pop si
    pop ax
    pop es
    pop bx
    pop bp
    ret 6
endp handleBuffer

proc printBuffer
    push bp
    mov bp, sp
    push ds
    push es
    push ax
    push bx

    mov ax, PRINT_LOCATION
    mov ds, ax

    cmp ax, VIDEO_MEMORY
    jz .dontPrint

    mov ax, VIDEO_MEMORY
    mov es, ax ; es -> video memory

    mov bx, 64000
.printLoop:

    mov al, [ds:bx] ; al -> pixel from buffer
    mov [es:bx], al ; print it to the screen

    dec bx
    jnz .printLoop
.dontPrint:
    pop bx
    pop ax
    pop es
    pop ds
    pop bp
    ret
endp printBuffer

proc updateKeyboard
    push ax
    push ds
    push es
    push bx
    push si
    push cx

    mov bx, offset KeyStateTable ; bx -> cur offset
    mov ax, seg KeyStateTable
    mov ds, ax ; ds -> cur seg

    mov si, offset PreviousKeyStateTable ; si -> prev offset
    mov ax, seg PreviousKeyStateTable
    mov es, ax ; es -> prev seg

    mov cx, 256 ; ah -> counter

.updateLoop:
    mov al, [ds:bx] ; al -> cur
    mov [es:si], al ; set the prev to the current

    inc bx
    inc si
    dec cx
    jnz .updateLoop

    pop cx
    pop si
    pop bx
    pop es
    pop ds
    pop ax
    ret
endp updateKeyboard

