proc initAndUploadMouse
    push bp
    mov bp, sp
    push ax
    push bx
    push cx
    push dx

    mov ax, 3
    int 33h   ; cx -> x, dx -> y, bx -> button status
    shr cx, 1 ; convert x from 0639 to 0319 

    push [bp + 10] ; layer offset
    push [bp + 8]  ; layer segment
    push 1
    push cx
    push dx
    push [bp + 6]  ; mouse offset
    push [bp + 4]  ; mouse segment
    call initAndUploadControllerToLayers ;  init to the middle of the screen

    pop dx
    pop cx
    pop bx
    pop ax
    pop bp
    ret 8
endp initAndUploadMouse

proc setControllerXandY
    push bp
    mov bp, sp
    push ds
    push bx
    push ax

    push [bp + 10] ; controller pointer
    push [bp + 8]  ; controller segment
    push RenderComponentId
    call getControllerComponent
    pop bx ; bx -> render component
    pop ds ; ds -> render segment

    mov ax, [bp + 6] ; x
    mov [bx + RenderComponentPositionX], ax
    mov ax, [bp + 4] ; y
