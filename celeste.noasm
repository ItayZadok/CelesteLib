    push bx
    push ax

    mov bx, [bp + 6] ; controller
    mov ax, [bp + 4] ; segment

    push bx ; controller
    push ax ; segment
    call updateVelocity

    push bx ; controller
    push ax ; segment
    call applyVelocity

    pop ax
    pop bx
    pop bp
    ret 4
endp updateAndApplyVelocity

proc walk
