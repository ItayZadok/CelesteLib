proc getControllerYEdges
    push bp
    mov bp, sp
    push ax
    push bx
    push dx
    push di
    push ds

    mov bx, [bp + 6] ; controller
    mov ds, [bp + 4] ; segment

    push bx
    push ds
    call getControllerY
    pop ax ; top

    push bx
    push ds
    push RenderComponentId
    push RenderComponentHeight
    call getControllerWordValue
    pop di

    mov dx, ax
    add dx, di       ; bottom
    mov [bp + 6], ax
