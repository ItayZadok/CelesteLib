endp getControllerXEdges

; return the difference in the Y axis between two objects
; input - first controller, second controller
proc isYCollision
    push bp
    mov bp, sp
    push ax
    push bx
    push si
    push dx

    push [bp + 10] ; first controller
    push [bp + 8]  ; first segment
    call getControllerYEdges
    pop ax ; bottom 1
    pop dx ; top 1

    push [bp + 6] ; second controller
    push [bp + 4] ; second segment
    call getControllerYEdges
    pop bx ; bottom 2
    pop si ; top 2

    push ax ; bottom 1
    push dx ; top 1
    push bx ; bottom 2
    push si ; top 2
    call isAxisCollisionSquare
    pop ax ; overlap

    mov [bp + 10], ax

    pop dx
    pop si
    pop bx
    pop ax
    pop bp
    ret 6
endp isYCollision

; return the difference in the X axis between two objects
; input - first controller, second controller
