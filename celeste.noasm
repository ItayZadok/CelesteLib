proc areControllersColliding
    push bp
    mov bp, sp
    push ax
    push bx
    push si
    push cx
    push di
    push dx
    push ds

    push [bp + 10] ; controller
    push [bp + 8]  ; segment
    push RenderComponentId
    call getControllerComponent
    pop di ; di -> render component
    pop ds ; component segment

    mov ds, [bp + 8]  ; controller segment
    push [di + RenderComponentPosition] ; save original position

    push 0
    push [di + RenderComponentPosition]
    push [di + RenderComponentPreviousPosition]
    call getStepElements
    pop cx ; diffX
    pop dx ; diffY
    pop ax ; ax -> amount of steps

    ; if no movement, don't raycast
    cmp ax, 0
    jne .startRaycast

    push [bp + 10] ; controller
    push [bp + 8]  ; controller segment
    push [bp + 6]  ; collider
    push [bp + 4]  ; collider segment
    call areControllersCollidingThisFrame
    pop ax ; collision overlap
    jmp .cleanup

.startRaycast:
    ; stepX = diffX / steps
    push cx
    push ax
    call divide
    pop cx ; store stepX

    ; stepY = diffY / steps
    push dx
    push ax
    call divide
    pop dx ; store stepY

    ; start raycast from previous position
    mov bx, [di + RenderComponentPreviousPosition]
    mov [di + RenderComponentPosition], bx

    xor bx, bx ; reset sumX
    xor si, si ; reset sumY

.raycastLoop:
    push cx ; store cx
    push dx ; store dx

    add bx, cx ; sumX += step X
    add si, dx ; sumY += step Y

    push bx ; sumX
    push RaycastPrecision
    call divideModulo
    pop cx  ; addX
    pop bx  ; updated sumX

    push si ; sumY
    push RaycastPrecision
    call divideModulo
    pop dx  ; addY
    pop si  ; updated sumY

    push cx ; addX
    push dx ; addY
    call toPosition
    pop cx  ; step size (ScreenWidth * y) + x

    add [di + RenderComponentPosition], cx ; apply the step

    pop dx ; restore dx
    pop cx ; restore cx

    push ax ; store ax (steps left)

    ; check collision at current position
    push [bp + 10] ; controller
    push [bp + 8]  ; controller segment
    push [bp + 6]  ; collider
    push [bp + 4]  ; collider segment
    call areControllersCollidingThisFrame
    pop ax ; ax -> collision overlap

    ; push [bp + 10]
    ; push [bp + 8]
    ; push 20
    ; call printControllerByColor ; to visualize

    cmp ax, 0
    je .didntCollide

    pop dx ; restore ax (steps left)
    jmp .cleanup

.didntCollide:
    pop ax ; restore ax (steps left)
    dec ax
    jnz .raycastLoop
    mov ax, 0

.cleanup:
    pop bx ; get original position
    cmp ax, 0 ; if no collision detect, restore the position
    jne .didCollide
    mov [di + RenderComponentPosition], bx

.didCollide:

    mov [bp + 10], ax ; return the collision overlap

    pop ds
    pop dx
    pop di
    pop cx
    pop si
    pop bx
    pop ax
    pop bp
    ret 6
endp areControllersColliding

