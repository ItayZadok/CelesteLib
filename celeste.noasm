    mov [bx + PhysicsComponentVelocityY], ax
    jmp .end

.bounceX:
    mov ax, [bx + PhysicsComponentVelocityX]
    neg ax
    mov [bx + PhysicsComponentVelocityX], ax
.end:

    pop ds
    pop ax
    pop bx
    pop bp
    ret 8
endp handleBounceCollision

