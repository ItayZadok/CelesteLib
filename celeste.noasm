proc isInRange
    push bp
    mov bp, sp
    push ax

    mov ax, [bp + 4] ; value

    cmp ax, [bp + 8] ; if value < min
    jl .false

    cmp ax, [bp + 6]  ; if value > max
    jg .false

    mov ax, 1   ; return true
    jmp .end

.false:
    xor ax, ax
.end:
    mov [bp + 8], ax
    pop ax
    pop bp
    ret 4
endp isInRange

