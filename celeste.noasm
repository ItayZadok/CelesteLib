    push 1
    push PhysicsComponentId
    push PhysicsComponentVelocityX
    call addControllerWordValue

    jmp .end

.skipPositive:

    push offset ballController
    push seg ballController
    push -1
    push PhysicsComponentId
    push PhysicsComponentVelocityX
    call addControllerWordValue

.end:

    ret
endp handleBallAcceleration

proc printPoint
    push bp
    mov bp, sp
    push ax
    push es
    push di

    mov ax, VideoMemory
    mov es, ax

    mov di, [bp + 6] ; pos
    mov ax, [bp + 4] ; color

    mov [byte ptr es:di], al

    pop di
    pop es
    pop ax
    pop bp
    ret 4
endp printPoint

proc isClicking
    push bp
    mov bp, sp
    push ax

    push [bp + 10] ; mouse controller
    push [bp + 8] ; mouse segment
    push MouseComponentId
    push MouseComponentLeftClicked
    call getControllerByteValue
    pop ax

    cmp al, 0
    je .end ; if did not click

    push [bp + 10] ; mouse controller
    push [bp + 8] ; mouse segment
    push RenderComponentId
    push RenderComponentPosition
    call getControllerWordValue
    pop ax

    push [bp + 6] ; button controller
    push [bp + 4] ; button segment
    push ax ; mouse position
    call doesPointCollideController
    pop ax  ; result (1/0)

.end:
    mov [bp + 10], ax

    pop ax
    pop bp
    ret 6
endp isClicking

proc executeProcIfTrue
    push bp
    mov bp, sp
    push ds

    cmp [bp + 8], 0 ; if not true
    je .end

    mov ds, [bp + 4] ; proc segment
    call [bp + 6]    ; call the proc

.end:
    pop ds
    pop bp
    ret 6
endp executeProcIfTrue

proc updateRandomSeed
    push bp
    mov bp, sp
    push bx

    mov bx, [bp + 4] ; seed offset
    
    push [bx]
    call getRandomWordBySeed
    pop [bx]

    pop bx
    pop bp
    ret 2
endp updateRandomSeed

proc gameLoop

    push offset mouseController
    push seg mouseController
    call updateMouse

    push offset rightPlayerController
    push seg rightPlayerController
    call handleRightPlayer

    push offset leftPlayerController
    push seg leftPlayerController
    call handleLeftPlayer

    push offset leftPlayerController
    push seg leftPlayerController
    call updateAndApplyVelocity

    push offset rightPlayerController
    push seg rightPlayerController
    call updateAndApplyVelocity

    push offset ballController
    push seg ballController
    call updateAndApplyVelocity

    push offset ballController
    push seg ballController
    push offset borderDown
    push seg borderDown
    call handleBounceCollision

    push offset ballController
    push seg ballController
    push offset borderUp
    push seg borderUp
    call handleBounceCollision

    push offset ballController
    push seg ballController
    push offset rightPlayerController
    push seg rightPlayerController
    call handleBounceCollision

    push offset ballController
    push seg ballController
    push offset leftPlayerController
    push seg leftPlayerController
    call handleBounceCollision

    push offset printManager
    push seg printManager
    push offset ballController
    push seg ballController
    call uploadController

    push offset printManager
    push seg printManager
    push offset leftPlayerController
    push seg leftPlayerController
    call uploadController

    push offset printManager
    push seg printManager
    push offset rightPlayerController
    push seg rightPlayerController
    call uploadController

    push offset printManager
    push seg printManager
    push offset mouseController
    push seg mouseController
    call uploadController

    push offset printManager
    push seg printManager
    call printAllControllers

    cmp [globalTime], gameSpeed
    jne .skip
    call handleBallAcceleration
.skip:

    ret
endp gameLoop

start:
    mov ax, @data
    mov ds, ax
    mov ax, 13h
    int 10h

    xor ax, ax
    xor dx, dx
    xor bx, bx
    xor si, si
    xor cx, cx
    xor di, di

    call clearScreen

    push offset palette
    call setPalette

; *******************INIT***************
; Note - here order is important, because they save their background here

; **************************************

    push offset backgroundImage
    push seg backgroundImage
    push 0
    call printImage

    push offset borderUp
    push seg borderUp
    push 20
    call printControllerByColor

    push offset borderDown
    push seg borderDown
    push 20
    call printControllerByColor

    push offset borderRight
    push seg borderRight
    push 20
    call printControllerByColor

    push offset borderLeft
    push seg borderLeft
    push 20
    call printControllerByColor

    push offset rightPlayerController
    push seg rightPlayerController
    push (ScreenWidth * 80) + 315
    call initController

    push offset leftPlayerController
    push seg leftPlayerController
    push (ScreenWidth * 80)
    call initController

    push offset ballController
    push seg ballController
    push (ScreenWidth * 125) + 160
    call initController

    push offset mouseController
    push seg mouseController
    call initMouse

    push offset oldKeyboardHandlerSegment
    push offset oldKeyboardHandlerOffset
    call setupKeyboardHandler

    push offset previousIRQMask
    call setUpClock

keepAliveLoop:
    jmp keepAliveLoop
exit:
    call endAndCleanUp
END start
