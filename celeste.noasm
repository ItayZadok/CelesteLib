;proc that calculates pseudo random numbers (without waiting)
; based on LFSR https://en.wikipedia.org/wiki/Linear-feedback_shift_register#Xorshift_LFSRs
proc getRandomWordBySeed
    push bp
    mov bp, sp
    push ax
    push cx
    push bx
    mov bx, 0
generate_rand:
	mov ax, [bp + 4]
	mov cx, ax
	and cx, 1
	shl cx, 2
	xor ax, cx
	mov cx, ax
	and cx, 100b
	shl cx, 1
	xor ax, cx
	mov cx, ax
	and cx, 1000b
	shl cx, 2
	xor ax, cx
	and ax, 100000b
	shl ax, 10
	shr [word ptr bp + 4], 1
	or [bp + 4], ax
	inc bx
	cmp bx, 3
	jne generate_rand
    pop bx
    pop cx
    pop ax
    pop bp
    ret
endp getRandomWordBySeed

