proc periodicGamePage

    call checkRestart
    call checkEscape

    push offset animationTime
    push seg animationTime
    call updateAnimationTime

    push offset animationLayer
    push seg animationLayer
    push [animationTime]
    call updateAnimationLayer

    push offset rightPlayerController
    push seg rightPlayerController
    call handleRightPlayer

    push offset leftPlayerController
    push seg leftPlayerController
    call handleLeftPlayer

    push offset physicsLayer
    push seg physicsLayer
    call updateAndApplyVelocityLayer
    
    push offset ballController
    push seg ballController
    push offset collisionLayer
    push seg collisionLayer
    call handleBounceCollisionLayer

    push offset renderLayer
    push seg renderLayer
    call printAllControllers

    push offset ballController
    push seg ballController
    push offset leftBorder
    push seg leftBorder
    call didControllerCollide
    pop ax
    cmp ax, 0
    jz .didntCollideLeft

    cmp [scoreLeft], 4
    jne .notLeftWin

    call resetGamePageScores
    push offset pageManager
    push seg pageManager
    push START_PAGE
    call switchPage
    jmp .end

.notLeftWin:

    inc [scoreLeft]
    push offset leftScoreLabel
    push seg leftScoreLabel
    push [scoreLeft]
    call setLabelByNumber

    call initGamePage

.didntCollideLeft:

    push offset ballController
    push seg ballController
    push offset rightBorder
    push seg rightBorder
    call didControllerCollide
    pop ax
    cmp ax, 0
    jz .didntCollideRight

    cmp [scoreRight], 4
    jne .notRightWin

    call resetGamePageScores
    push offset pageManager
    push seg pageManager
    push START_PAGE
    call switchPage
    jmp .end

.notRightWin:

    inc [scoreRight]
    push offset rightScoreLabel
    push seg rightScoreLabel
    push [scoreRight]
    call setLabelByNumber

    call initGamePage

.didntCollideRight:

    cmp [word ptr globalTime], GAME_SPEED
    jnz .end ; once per second
    call accelerateBall
.end:

    ret
endp periodicGamePage

proc resetGamePageScores

    mov [scoreLeft], 0
    mov [scoreRight], 0

    push offset leftScoreLabel
    push seg leftScoreLabel
    push [scoreLeft]
    call setLabelByNumber

    push offset rightScoreLabel
    push seg rightScoreLabel
    push [scoreRight]
    call setLabelByNumber

    ret
