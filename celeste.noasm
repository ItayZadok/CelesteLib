    dec [word ptr bx + ArraySize]            ; decrease array size
    mul dx                          ; ax = index * element size

    add bx, ArrayData ; first element
    add bx, ax        ; currect element position

    mov bx, [bx]
    mov [bp + 4], bx

    pop ax
    pop dx
    pop bx
